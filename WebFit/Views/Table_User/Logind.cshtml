
@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en">
<head>
    <title>Login V1</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="http://cstee.kru.ac.th/Cs59/59123250105/Login/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="http://cstee.kru.ac.th/Cs59/59123250105/Login/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="http://cstee.kru.ac.th/Cs59/59123250105/Login/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="http://cstee.kru.ac.th/Cs59/59123250105/Login/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="http://cstee.kru.ac.th/Cs59/59123250105/Login/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="http://cstee.kru.ac.th/Cs59/59123250105/Login/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="http://cstee.kru.ac.th/Cs59/59123250105/Login/css/util.css">
    <link rel="stylesheet" type="text/css" href="http://cstee.kru.ac.th/Cs59/59123250105/Login/css/main.css">
    <!--===============================================================================================-->
</head>
<body>


    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100">
                <div class="login100-pic js-tilt" data-tilt>
                    <img src="~/Login/images/Kru.png" alt="IMG">
                </div>

                <form class="login100-form validate-form">
                    <span class="login100-form-title">
                        สมัครสมาชิก
                    </span>

                    <div class="wrap-input100 validate-input" data-validate="Valid email is required: ex@abc.xyz">
                        <input class="input100" type="text" name="Username" placeholder="ชื่อผู้ใช้">
                        <span class="focus-input100"></span>
                        <span class="symbol-input100">
                            <i class="fa fa-envelope" aria-hidden="true"></i>
                        </span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Password is required">
                        <input class="input100" type="password" name="Password" placeholder="รหัสผ่าน">
                        <span class="focus-input100"></span>
                        <span class="symbol-input100">
                            <i class="fa fa-lock" aria-hidden="true"></i>
                        </span>
                    </div>

                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn">
                            ล้อกอิน
                        </button>
                    </div>

                    @*<div class="text-center p-t-12">
                        <span class="txt1">
                            Forgot
                        </span>
                        <a class="txt2" href="#">
                            Username / Password?
                        </a>
                    </div>*@

                    <div class="text-center p-t-136">
                        <a class="txt2" href="../Table_User/Create">
                            สมัครสมาชิก
                            <i class="fa fa-long-arrow-right m-l-5" aria-hidden="true"></i>
                        </a>
                    </div>
                </form>
            </div>
        </div>
    </div>






    <!--===============================================================================================-->
    <script src="http://cstee.kru.ac.th/Cs59/59123250105/Login/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="http://cstee.kru.ac.th/Cs59/59123250105/Login/vendor/bootstrap/js/popper.js"></script>
    <script src="http://cstee.kru.ac.th/Cs59/59123250105/Login/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="http://cstee.kru.ac.th/Cs59/59123250105/Login/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="http://cstee.kru.ac.th/Cs59/59123250105/Login/vendor/tilt/tilt.jquery.min.js"></script>
    <script>
        $('.js-tilt').tilt({
            scale: 1.1
        })
    </script>
    <!--===============================================================================================-->
    <script src="http://cstee.kru.ac.th/Cs59/59123250105/Login/js/main.js"></script>

</body>
</html>
